{
  "manifest_version": 3,
  "name": "Zoho People Überstunden Berechnung",
  "version": "0.13",
  "description": "Berechnet die tatsächlichen Überstunden in Zoho People",
  "author": "Markwart v. Bonin",
  "content_scripts": [
    {
      "matches": [
        "https://people.zoho.com/*/zp*",
        "https://people.ridecore.com/*/zp*",
        "https://people.zoho.com/*/zp#reports/*/attendance/user",
        "https://people.ridecore.com/*/zp#reports/*/attendance/user"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
